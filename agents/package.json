{
  "name": "mpa-deal-intelligence-agents",
  "version": "1.0.0",
  "description": "Multi-agent system for M&A deal intelligence - Morrison Park Advisors",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prospector": "npx tsx src/agents/prospector.ts",
    "researcher": "npx tsx src/agents/researcher.ts",
    "connector": "npx tsx src/agents/connector.ts",
    "orchestrator": "npx tsx src/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.52.0",
    "@supabase/supabase-js": "^2.45.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
